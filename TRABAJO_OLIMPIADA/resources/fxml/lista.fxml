<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.text.Font?>

<GridPane maxHeight="-Infinity" maxWidth="-Infinity"
	minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0"
	prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1"
	xmlns="http://javafx.com/javafx/18" fx:controller="controller.listaController">
	<columnConstraints>
		<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0"
			prefWidth="100.0" />
		<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0"
			prefWidth="100.0" />
	</columnConstraints>
	<rowConstraints>
		<RowConstraints minHeight="10.0" prefHeight="30.0"
			vgrow="SOMETIMES" />
		<RowConstraints maxHeight="266.0" minHeight="10.0"
			prefHeight="266.0" vgrow="SOMETIMES" />
		<RowConstraints maxHeight="126.0" minHeight="0.0"
			prefHeight="0.0" vgrow="SOMETIMES" />
	</rowConstraints>
	<children>
		<Label fx:id="lblTitulo" alignment="CENTER"
			text="Lista de Objetos" GridPane.columnSpan="2"
			GridPane.halignment="CENTER">
			<font>
				<Font size="35.0" />
			</font>
		</Label>
		<ListView fx:id="listObjetos" prefHeight="200.0"
			prefWidth="200.0" GridPane.columnSpan="2" GridPane.rowIndex="1" />
		<FlowPane alignment="CENTER" prefHeight="200.0"
			prefWidth="200.0" GridPane.columnSpan="2" GridPane.rowIndex="2">
			<children>
				<Button fx:id="btnAceptar" mnemonicParsing="false"
					onAction="#Aceptar" text="Aceptar" />
				<Button fx:id="btnCancelar" mnemonicParsing="false"
					onAction="#cancelar" text="Cancelar">
					<FlowPane.margin>
						<Insets left="50.0" />
					</FlowPane.margin>
				</Button>
			</children>
		</FlowPane>
	</children>
</GridPane>
